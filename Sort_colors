class Solution {
public:
    void sortColors(vector<int>& nums) {
        int x=nums.size();
        for(int i=0;i<x-1;i++)
        {
            int min=i;
            for(int j=i+1;j<x;j++)
            {
                if(nums[j]<nums[min])
                {
                    min=j;
                }
            }
            int temp=nums[i];
            nums[i]=nums[min];
            nums[min]=temp;
        }
    }
};
